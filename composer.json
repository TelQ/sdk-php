{
  "name": "telq/sdk",
  "description": "SDK for telq REST API",
  "keywords": ["sdk"],
  "homepage": "https://github.com/TelQ/sdk-php",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.6.3 || ^9.5.20"
  },
  "autoload": {
    "psr-4": {
      "TelQ\\Sdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TelQ\\Sdk\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --verbose tests"
  }
}